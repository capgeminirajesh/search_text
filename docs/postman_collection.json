{
  "info": {
    "name": "Drive Search Service",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/health",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "health"
          ]
        }
      }
    },
    {
      "name": "Sync",
      "request": {
        "method": "POST",
        "url": {
          "raw": "{{baseUrl}}/sync",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "sync"
          ]
        }
      }
    },
    {
      "name": "Search",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/search?q=project&limit=10",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "search"
          ],
          "query": [
            {
              "key": "q",
              "value": "project"
            },
            {
              "key": "limit",
              "value": "10"
            }
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://127.0.0.1:8000"
    }
  ]
}
